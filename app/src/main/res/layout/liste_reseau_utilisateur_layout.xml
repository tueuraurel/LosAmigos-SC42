<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Mes Reseaux" />

    <Button
        android:text="@string/ajouterUnReseau"
        android:id="@+id/boutonVersCreationReseau"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <Button
        android:text="Rechercher un reseau"
        android:id="@+id/boutonVersRechercheReseau"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <ListView
        android:id="@+id/listeViewReseauUtilisateur"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        />


</LinearLayout>
